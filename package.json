{
  "name": "mind-elixir",
  "version": "2.1.0",
  "type": "module",
  "description": "Mind elixir is a free open source mind map core.",
  "keywords": [
    "mind-elixir",
    "mindmap",
    "dom",
    "visualization"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --cache --max-warnings 0  \"src/**/*.{js,json,ts}\" --fix",
    "dev": "vite",
    "build": "node build.js && tsc",
    "preview": "vite preview",
    "test": "jest --runInBand",
    "doc": "./node_modules/.bin/jsdoc src/ -R readme.md -c conf.json",
    "md": "./node_modules/.bin/jsdoc2md 'src/*.js' > api.md",
    "publish2": "npm run build && npm version patch -m \"build: release %s\" && npm publish",
    "beta": "npm run build && npm publish --tag beta"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/MindElixir.js",
      "require": "./dist/MindElixir.js"
    },
    "./example": {
      "types": "./dist/types/exampleData/1.d.ts",
      "import": "./dist/example.js",
      "require": "./dist/example.js"
    },
    "./readme.md": "./readme.md",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "example": [
        "./dist/types/exampleData/1.d.ts"
      ]
    }
  },
  "main": "dist/MindElixir.js",
  "types": "dist/types/index.d.ts",
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --cache --fix"
    ],
    "src/**/*.{json,less}": [
      "prettier --write"
    ]
  },
  "files": [
    "package.json",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ssshooter/mind-elixir-core"
  },
  "homepage": "https://mind-elixir.com/",
  "author": "ssshooter",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "better-docs": "^2.7.2",
    "docdash": "^1.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jsdoc": "^3.6.11",
    "jsdoc-to-markdown": "^7.1.1",
    "less": "^4.1.3",
    "less-loader": "^10.2.0",
    "lint-staged": "^13.2.3",
    "prettier": "2.8.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.8"
  },
  "dependencies": {
    "vite-plugin-css-injected-by-js": "^3.2.1"
  }
}